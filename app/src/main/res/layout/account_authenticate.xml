<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#FFFFFF"
    tools:context=".AuthenticateFragment">

    <LinearLayout
        android:layout_width="406dp"
        android:layout_height="84dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="2dp"
        android:background="@color/colorPrimary"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="201dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:text="WELCOME!"
                android:gravity="center"
                android:textColor="#F8F5F5" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="160dp"
                android:layout_height="47dp"
                android:text="Login or Sign up to access all services offered by Nabscom"
                android:layout_gravity="center"
                android:textColor="#FFFFFF" />
        </LinearLayout>

        <Button
            android:id="@+id/signupbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="#FFFFFF"
            android:text="SIGNUP" />

        <Button
            android:id="@+id/login_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:backgroundTint="#F7F8F8"
            android:text="LOGIN" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/account"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="88dp"
            android:layout_marginEnd="341dp"
            android:text="ACCOUNT SERVICES"
            android:gravity="bottom"
            android:textColor="@android:color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:layout_width="369dp"
            android:layout_height="611dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="11dp"
            android:layout_marginEnd="32dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/account">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/order"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_book_24" />

                <TextView
                    android:id="@+id/order_txt"
                    android:layout_width="258dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Orders"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/order_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/pending"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_autorenew_24" />

                <TextView
                    android:id="@+id/pending_txt"
                    android:layout_width="261dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Pending Reviews"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/pending_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/saved"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_favorite_24" />

                <TextView
                    android:id="@+id/saved_txt"
                    android:layout_width="261dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Saved Items"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/saved_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/viewed"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_fast_rewind_24" />

                <TextView
                    android:id="@+id/viewed_txt"
                    android:layout_width="261dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Recently Viewed"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/viewed_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/transit"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_emoji_transportation_24" />

                <TextView
                    android:id="@+id/transit_txt"
                    android:layout_width="261dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="In Transit"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/transit_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
            </LinearLayout>

            <TextView
                android:id="@+id/tools"
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:gravity="bottom"
                android:text="TOOLS"
                android:textColor="@android:color/black" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="54dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/address"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_location_city_24" />

                <TextView
                    android:id="@+id/address_txt"
                    android:layout_width="261dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Address Book"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/address_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/details"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_accessibility_new_24" />

                <TextView
                    android:id="@+id/details_txt"
                    android:layout_width="261dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Details"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/details_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="46dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/settings"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    app:itemIconTint="@android:color/black"
                    android:src="@drawable/ic_baseline_settings_24" />

                <TextView
                    android:id="@+id/settings_txt"
                    android:layout_width="261dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Settings"
                    android:gravity="bottom"
                    android:textColor="@android:color/black" />

                <ImageButton
                    android:id="@+id/settings_img"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_baseline_arrow_forward_ios_24" />

            </LinearLayout>
        </LinearLayout>
        <FrameLayout
            android:id="@+id/frameContainer"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            tools:ignore="MissingConstraints" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>